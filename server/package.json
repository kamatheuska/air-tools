    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.17.5",
    "mongodb": "^3.0.2",
    "mongoose": "^5.0.3",
    "morgan": "^1.9.0",
    "validator": "^9.4.0"
  },
  "devDependencies": {
    "eslint-config-airbnb-base": "^12.1.0",
